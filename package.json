{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for React, Redux, Express based on react-slingshot",
  "engines": {
    "npm": ">=3",
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "postbuild": "babel tools/distServer.js --out-file dist/distServer.js && cp -r src/images/*.* dist/images/",
    "clean-dist": "npm run remove-dist && mkdir dist && mkdir dist/images",
    "remove-dist": "rimraf ./dist",
    "start": "node dist/distServer.js",
    "start:dev": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Hai Pham",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.4",
    "express-healthcheck": "0.1.0",
    "helmet": "3.14.0",
    "history": "4.7.2",
    "object-assign": "4.1.1",
    "pre-commit": "1.2.2",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "react-redux-i18n": "1.9.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "version-healthcheck": "0.1.2",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "babel-preset-react-hmre": "1.1.1",
    "browser-sync": "2.26.3",
    "chai": "4.2.0",
    "chalk": "2.4.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "5.8.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "npm-run-all": "4.1.3",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "4.3.12",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.0.0",
    "rimraf": "2.6.2",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.24.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/tools/enzyme.js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
