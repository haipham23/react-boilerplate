{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for React, Redux, Express based on react-slingshot",
  "engines": {
    "npm": ">=3",
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "postbuild": "babel tools/distServer.js --out-file dist/distServer.js && cp -r src/images/*.* dist/images/",
    "clean-dist": "npm run remove-dist && mkdir dist && mkdir dist/images",
    "remove-dist": "rimraf ./dist",
    "start": "node dist/distServer.js",
    "start:dev": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Hai Pham",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.3",
    "express-healthcheck": "0.1.0",
    "helmet": "3.12.1",
    "history": "4.7.2",
    "object-assign": "4.1.1",
    "pre-commit": "1.2.2",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-redux-i18n": "1.9.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "version-healthcheck": "0.1.2",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.24.5",
    "chai": "4.1.2",
    "chalk": "2.4.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "4.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.1",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "npm-run-all": "4.1.3",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "4.3.3",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.0.0",
    "rimraf": "2.6.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "4.16.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/tools/enzyme.js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
